<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kota203.quizmuseumgeologi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".pilihAktorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginGuruActivity"></activity>

        <activity android:name=".ManajemenKuisActivity"
            android:parentActivityName=".LoginGuruActivity"></activity>

        <activity android:name=".MulaiPermaiananActivity"
            android:parentActivityName=".ManajemenKuisActivity"></activity>

        <activity android:name=".Interface.Peserta.LoginPesertaActivity"></activity>

        <activity android:name=".Interface.Peserta.InfoKelompokActivity"
            android:parentActivityName=".Interface.Peserta.LoginPesertaActivity"></activity>

        <activity android:name=".Interface.Peserta.PetunjukActivity"></activity>

    </application>

</manifest>